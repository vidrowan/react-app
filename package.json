{
  "name": "react-template",
  "version": "1.0.0",
  "description": "template react app",
  "author": "andrewgrewell@gmail.com (http://www.andrewgrewell.com/)",
  "repository": {
    "type": "git",
    "url": "git@github.com:andrewgrewell/react-app.git"
  },
  "scripts": {
    "build:node-sass": "./node_modules/.bin/node-sass -o ./server/public/styles --output-style compressed ./server/public/styles/base_styles.scss",
    "build:webpack": "NODE_ENV=production webpack --config webpack.config.prod.js --display-error-details",
    "build": "npm run build:node-sass && npm run build:webpack",
    "start": "node initServer.js",
    "lint": "eslint app",
    "test": "jest"
  },
  "license": "CC0-1.0",
  "jest": {
    "scriptPreprocessor": "<rootDir>/jest/preprocessor.js",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/react-addons-test-utils",
      "<rootDir>/node_modules/fbjs",
      ".+_styles.*"
    ],
    "testFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx"
    ],
    "verbose": true
  },
  "jest-webpack-alias": {
    "configFile": "webpack.config.dev.js"
  },
  "devDependencies": {
    "babel-jest": "^5.3.0",
    "eslint": "^1.3.1",
    "eslint-plugin-react": "^2.3.0",
    "jest-cli": "^0.8.2",
    "jest-webpack-alias": "^2.2.0",
    "react-addons-test-utils": "^0.14.6",
    "react-hot-loader": "^1.3.0",
    "rimraf": "^2.4.3",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-hot-middleware": "^2.0.0"
  },
  "dependencies": {
    "babel-core": "^5.4.7",
    "babel-eslint": "^3.1.9",
    "babel-loader": "^5.1.2",
    "bluebird": "^3.1.1",
    "classnames": "^2.2.0",
    "compass-mixins": "^0.12.7",
    "css-loader": "^0.19.0",
    "express": "^4.13.3",
    "history": "^1.17.0",
    "hjs": "0.0.6",
    "immutable": "^3.7.5",
    "jquery": "^2.2.0",
    "mocha": "^2.3.4",
    "node-sass": "^3.4.2",
    "raw-loader": "^0.5.1",
    "react": "^0.14.6",
    "react-dom": "^0.14.6",
    "react-redux": "^4.0.0",
    "react-router": "^1.0.3",
    "redux": "^3.0.4",
    "sass-loader": "^3.1.1",
    "serve-favicon": "^2.3.0",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.11"
  }
}
